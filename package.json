{
  "name": "team2018",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve --c=production",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build -c=production && ng run team2018:server",
    "webpack:server": "webpack --mode development --config webpack.server.config.js --progress --colors",
    "ngc-app-watch": "ngc -w -p src/tsconfig.app.json",
    "ngc-app": "ngc -p src/tsconfig.app.json",
    "rollup-clean-dev": "rimraf out-tsc && rimraf dist-rollup",
    "rollup-clean-prod": "rimraf out-tsc && rimraf dist-rollup-prod",
    "rollup-watch": "rollup -c -w --environment BUILD:dev",
    "rollup-dev": "npm run rollup-clean-dev && npm run ngc-app && npm run rollup-dev-app",
    "rollup-dev-app": "npm-run-all --parallel ngc-app-watch rollup-watch",
    "rollup-prod": "npm run rollup-clean-prod && npm run ngc-app && rollup -c --environment BUILD:prod",
    "rollup-serve": "rollup -c --environment BUILD:prod",
    "ngc-app-common": "ngc -p projects/app-common/tsconfig.lib.json",
    "rollup-app-common": "rollup --config projects/app-common/rollup.config.js --environment BUILD:prod",
    "ngc-dashboard": "ngc -p projects/dashboard/tsconfig.app.json",
    "rollup-dashboard": "rollup --config projects/dashboard/rollup.config.js --environment BUILD:prod",
    "ngc-plan": "ngc -p projects/plan/tsconfig.app.json",
    "rollup-plan": "rollup --config projects/plan/rollup.config.js --environment BUILD:prod",
    "ngc-objective": "ngc -p projects/objective/tsconfig.app.json",
    "rollup-objective": "rollup --config projects/objective/rollup.config.js --environment BUILD:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "^7.2.8",
    "@angular/pwa": "^0.12.4",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@progress/kendo-angular-buttons": "^4.3.2",
    "@progress/kendo-angular-dateinputs": "^3.5.1",
    "@progress/kendo-angular-dialog": "^3.10.0",
    "@progress/kendo-angular-dropdowns": "^3.4.0",
    "@progress/kendo-angular-excel-export": "^2.3.0",
    "@progress/kendo-angular-grid": "^3.12.0",
    "@progress/kendo-angular-inputs": "^4.0.0",
    "@progress/kendo-angular-intl": "^1.6.1",
    "@progress/kendo-angular-l10n": "^1.3.0",
    "@progress/kendo-angular-label": "^1.3.0",
    "@progress/kendo-angular-layout": "^3.2.0",
    "@progress/kendo-angular-popup": "^2.5.0",
    "@progress/kendo-data-query": "^1.5.0",
    "@progress/kendo-drawing": "^1.5.7",
    "@progress/kendo-theme-default": "^3.2.0",
    "bootstrap": "^4.2.0",
    "core-js": "^2.5.4",
    "immutable": "^4.0.0-rc.12",
    "lodash-es": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "ts-loader": "^5.3.3",
    "validator": "^10.9.0",
    "webpack-cli": "^3.2.3",
    "zone.js": "~0.8.26",
    "systemjs": "^3.1.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "^7.2.9",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.7.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-deep-scope-plugin": "^1.6.0",
    "browser-sync": "^2.26.3",
    "compression": "^1.7.4",
    "http2": "^3.3.7",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.7.4",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-hash": "^1.3.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-static-site": "0.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "strip-json-comments": "^2.0.1"
  }
}
