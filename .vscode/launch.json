  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "rollup-objective"
      ],
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "rollup objective",
      "program": "${workspaceFolder}/node_modules/.bin/rollup",
      "args": [
        "--config",
        "projects/objective/rollup.config.js",
        "--environment",
        "BUILD:prod"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/serve"
    }
  ]
}